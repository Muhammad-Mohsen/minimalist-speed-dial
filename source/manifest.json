{
"update_url": "https://clients2.google.com/service/update2/crx",

	"version": "1.0.1",
	"manifest_version": 2,

	"name": "Minimalist Speed Dial",
	"description": "Minimalist Speed Dial.",
	"browser_action": {
		"default_icon": "images/browser-action-icon-48.png"
	},

	"permissions": [
		"storage"
	],

	"icons": {
		"48": "images/speed-dial-icon-48.png",
		"128": "images/speed-dial-icon-128.png"
	},

	"chrome_url_overrides": {
		"newtab": "html/index.html"
	},
	"background": {
		"scripts": [
			"scripts/background-main.js"
		]
	}
}